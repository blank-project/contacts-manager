{{> contactFilters }}
<table class="contact-list">
    <thead>
      <tr>
        <th></th>
        <th>Nom</th>
        <th>Mail</th>
        <th>Téléphone</th>
        <th>Organisation</th>
        <th>Fonction</th>
        <th>Adresse</th>
        <th>Etiquettes</th>
      </tr>
    </thead>
    <tbody>
    {{#contacts}}
      <tr>
        <td><a href="/contacts/{{ id }}">See</a></td>
        <td>{{ fullName }}</td>
        <td>{{ email }}</td>
        <td>{{ phone }}</td>
        <td>{{ organization }}</td>
        <td>{{ title }}</td>
        <td>{{ formattedAddress }}</td>
        <td>
          {{#tags}}
            {{> tag }}
          {{/tags}}
        </td>
      </tr>
    {{/contacts}}
    </tbody>
</table>
<a href="edit">Nouveau</a>
