<table class="contact-list">
    <thead>
      <tr>
        <th></th>
        <th>Nom</th>
        <th>Mail</th>
        <th>Téléphone</th>
      </tr>
    </thead>
    <tbody>
    {{#contacts}}
      <tr>
        <td><a href="/contacts/{{ id }}">See</a></td>
