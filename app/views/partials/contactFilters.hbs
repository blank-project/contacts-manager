  <label for="tagId">Etiquettes</label>
  <select multiple name="tagId" id="tagId">
    {{#tags}}
    <option value="{{ id }}">{{ name }}</option>
    {{/tags}}
  </select><br />

  <input type="submit" value="Filtrer" />
</form>
